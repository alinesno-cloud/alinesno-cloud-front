

/*赠送代金券*/


.mask {
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}

.gx-popp {
  width: 606rpx;
  height: 505rpx;
  padding: 0 70rpx;
  padding-top: 157rpx;
  box-sizing: border-box;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  position: fixed;
  top: 20%;
  left: 50%;
  margin-left: -303rpx;
  z-index: 101;
}

.shining {
  width: 606rpx;
  height: 412rpx;
  position: absolute;
  top: -255rpx;
  left: 0;
}

.gx-title {
  margin-bottom: 50rpx;
  text-align: center;
  font-size: 32rpx;
  color: #785019;
}

.gx-title .big {
  font-size: 60rpx;
}

.used-p {
  margin-bottom: 20rpx;
  font-size: 14px;
  color: #e27d33;
}

.used-list {
  display: flex;
  justify-content: space-between;
  margin-bottom: 60rpx;
}

.used-img {
  width: 222rpx;
  height: 240rpx;
}

.gx-image {
  width: 222rpx;
  height: 240rpx;
}

.gx-btn {
  width: 450rpx;
  height: 88rpx;
  margin: 0 auto 20rpx;
  border-radius: 50rpx;
  background: -webkit-linear-gradient(top, #ff3b11, #f77f2e);
  background: -o-linear-gradient(bottom, #ff3b11, #f77f2e);
  background: -moz-linear-gradient(bottom, #ff3b11, #f77f2e);
  background: linear-gradient(to bottom, #ff3b11, #f77f2e);
  font-size: 16px;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
}

.who {
  font-size: 14px;
  color: #785019;
  text-align: center;
}

.gx-close {
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  bottom: -120rpx;
  left: 50%;
  margin-left: -28rpx;
}

.close-icon {
  width: 56rpx;
  height: 56rpx;
}

