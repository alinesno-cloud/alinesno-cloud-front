  left: 0;
  bottom: 0;
  z-index: 980;
}

.pro-btn {
  width: 120rpx;
  height: 100rpx;
  padding-top: 10rpx;
  text-align: center;
  font-size: 20rpx;
  color: #999;
  position: relative;
}

.pro-btn .iconfont {
  font-size: 20px;
}

.num-p {
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  background: #ff1919;
  line-height: 26rpx;
  color: #fff;
  position: absolute;
  top: 10rpx;
  right: 20rpx;
}

.addcart,.buy-now {
  flex: 1;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}

.addcart {
  background: #ff1919;
}

.buy-now {
  background: #4c4491;
}

.pro-btm-margin {
