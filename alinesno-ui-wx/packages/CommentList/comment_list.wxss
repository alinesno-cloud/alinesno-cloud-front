  font-size: 32rpx;
  color: #f00;
}

.team-man {
  margin-right: 10rpx;
  display: flex;
  align-items: center;
  color: #f32224;
  font-size: 24rpx;
}

.big-tit {
  padding: 0 15rpx;
  height: 75rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px #e6e6e6 solid;
}

.big-name {
  width: 145rpx;
  height: 75rpx;
  margin-right: 45rpx;
  text-align: center;
  line-height: 75rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  position: relative;
}

.big-name:after {
  display: block;
  content: '';
  width: 100%;
  height: 8rpx;
  background: #f00;
  position: absolute;
  bottom: 0;
  left: 0;
}

.pro-detail {
  padding: 40rpx 0;
  background: #fff;
}

.pro-btm {
  width: 100%;
