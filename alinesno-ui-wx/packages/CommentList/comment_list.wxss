}

.tuijian-item {
  flex: 0 0 210rpx;
  padding-right: 30rpx;
}

.tuijian-img {
  width: 180rpx;
  height: 180rpx;
  margin-bottom: 15rpx;
  border-radius: 15rpx;
  overflow: hidden;
}

