.to-scroll {
  width: 100%;
  height: 446rpx;
  overflow-y: scroll;
  border-bottom: 1rpx #e3e3e3 solid;
}

.to-content {
  padding: 0 30rpx;
}

.to-bar {
  padding: 35rpx 50rpx 35rpx 35rpx;
  border-bottom: 1rpx #ebebeb solid;
  font-size: 28rpx;
  color: #333;
  position: relative;
}



.to-bar.active {
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 36rpx 36rpx;
}



.to-link {
  width: 600rpx;
  height: 90rpx;
  border-radius: 50rpx;
  margin: 20rpx auto;
  font-size: 30rpx;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
}

.city-picker {
  width: 600rpx;
  height: 90rpx;
  position: absolute;
  bottom: 20rpx;
  left: 75rpx;
  z-index: 9;
  opacity: 0;
}

.city-picker .picker {
  height: 90rpx;
}

.xsdst-xz {
  width: 100%;
  height: 74rpx;
  position: relative;
  background: #fff;
  overflow: hidden;
  margin-bottom: 10px;
}

.set_default {
  color: #333;
}

.xsdst-xz picker {
  width: 100%;
  height: 100%;
  display: block;
  position: relative;
  z-index: 5;
  line-height: 74rpx;
  text-indent: 20rpx;
  font-size: 28rpx;
  color: #333;
}

.xsdstx-text {
  width: 100%;
  height: 100%;
  display: block;
  position: relative;
  z-index: 5;
  line-height: 74rpx;
  text-indent: 20rpx;
  font-size: 28rpx;
  color: #333;
}

.xsdstx-icon {
  position: absolute;
  z-index: 2;
  right: 0;
  top: 0;
  height: 100%;
  line-height: 74rpx;
  width: 29rpx;
  border-left: 1px solid #5dae51;
  background: #def7da;
}

.xsdstx-icon text {
  width: 100%;
  height: 100%;
  display: block;
  text-align: center;
  text-indent: 0;
  font-size: 26rpx;
}

.edit-ti {
  height: 64rpx;
  padding: 0 30rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  color: #333;
}

.edit-box {
  padding-left: 30rpx;
  background: #fff;
}

.edit-bar {
  display: flex;
}

.edit-name {
  width: 84rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #333;
}

.edit-txt {
  flex: 1;
  height: 80rpx;
  border-bottom: 1rpx #e6e6e6 solid;
}

.edit-txt input {
  width: 100%;
  height: 80rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-size: 28rpx;
}

.sex-bar {
  padding: 20rpx 0;
  font-size: 28rpx;
  color: #333;
}

.radio {
  margin-right: 20rpx;
}

.take-txt {
  height: 80rpx;
  background: #fff;
}

.take-txt input {
  width: 100%;
  height: 80rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
}

.btn-area {
  width: 100%;
  height: 100rpx;
  /* padding: 15rpx 30rpx; */
  box-sizing: border-box;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 98;
}


.del_btn {
  background: rgb(90, 88, 88);
}



.address-edit {
  z-index: 999;
  height: 745rpx;
}

.fl {
  width: 33%;
  float: left;
}

.btn-area button {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  border-radius: 0;
  float: left;
  padding: 0;
  font-weight: normal;
}


.citypicker {}

.citybody {
  position: fixed;
  bottom: 0px;
  background: #fff;
  z-index: 99999;
  width: 100%;
  height: 745rpx;
}

.cityheader {
  position: absolute;
  top: 0px;
  width: 100%;
  z-index: 99999;
}

.city-cancel {
  float: left;
  margin: 20rpx;
  color: #818181;
}

.city-true {
  float: right;
  margin: 20rpx;
  color: #da251d
}

.input {

  width: 530rpx;

}