@import "../../css/color.wxss";

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 991;
}

.to {
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 992;
}

.to-tit {
  height: 82rpx;
  line-height: 82rpx;
  border-bottom: 1rpx #d5d5d5 solid;
  text-align: center;
  font-size: 34rpx;
  color: #333;
}

.to-close {
  width: 82rpx;
  height: 82rpx;
  line-height: 82rpx;
  text-align: center;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 9;
}

.to-close image {
  width: 50rpx;
  height: 50rpx;
  margin-top: 17rpx;

}

.to-back image {
  width: 50rpx;
  height: 50rpx;
  margin-top: 17rpx;

}

.to-back {
  width: 82rpx;
  height: 82rpx;
  line-height: 82rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
  font-size: 16px;
}

